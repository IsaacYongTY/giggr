{
    "name": "learn-starter",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "test": "jest --config ./jest.config.js",
        "test:dev": "jest --config ./jest.config.js --watchAll",
        "test:ci": "jest --verbose",
        "test:coverage": "yarn test --coverage",
        "storybook": "start-storybook -p 6006 -s ./public",
        "build-storybook": "build-storybook",
        "prepare": "husky install",
        "lint:prettier": "prettier --check './**/*.(ts|tsx|js|jsx)'",
        "lint:eslint": "eslint --max-warnings 0 './**/*.?(ts|tsx|js|jsx)'",
        "lint": "tsc --noEmit -p . && npm run lint:prettier && npm run lint:eslint"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "prettier --write",
            "eslint --fix",
            "git add"
        ],
        "*.{html,css,less,ejs}": [
            "prettier --write",
            "git add"
        ]
    },
