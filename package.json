        "axios": "^0.21.1",
        "babel-jest": "^27.0.6",
        "babel-loader": "^8.2.2",
        "babel-plugin-transform-import-ignore": "^1.1.0",
        "chinese-to-pinyin": "^1.3.1",
        "classnames": "^2.3.1",
        "contains-chinese": "^1.0.0",
